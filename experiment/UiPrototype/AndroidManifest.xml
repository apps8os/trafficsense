<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uiprototype"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <!-- Permissions required by ContextLogger3 -->
    <!-- Launching -->
    <uses-permission android:name="android.permission.BATTERY_STATS" />
    <!-- All probes -->
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <!-- Storage -->
    <!-- Config downloading/Data uploading -->
    <!-- WifiProbe -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <!-- CellProbe -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    
    <!-- Alarm test -->
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
	<uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.uiprototype.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.uiprototype.MapActivity"
            android:label="@string/title_activity_map" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
		    android:name="com.google.android.gms.version"
		    android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyBaj4ulWshoLduXGDhQc2oVuGCSxni85-Q"/>
        <service android:name="org.apps8os.trafficsense.android.TimeOnlyService" />
        <!-- ContextLogger3 related services -->
        <!-- framework core -->
        <service android:name="org.apps8os.contextlogger.android.io.MainPipeline"/>
        <service android:name="edu.mit.media.funf.storage.HttpUploadService"/>
        <service android:name="edu.mit.media.funf.storage.NameValueDatabaseService"/>
        <!-- Probes -->
        <service android:name="edu.mit.media.funf.probe.builtin.CellProbe" />
        <service android:name="edu.mit.media.funf.probe.builtin.WifiProbe" />
        <!-- Please ignore the warning saying use '$' instead of '.', it breaks stuff. -->
        <service android:name="org.apps8os.contextlogger.android.CustomProbe.ApplicationProbe" />
    </application>

</manifest>
